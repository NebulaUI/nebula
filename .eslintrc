{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "comma-dangle": ["error", "never"],
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "semi": ["error", "never"],
    "react/require-default-props": "off",
    "react/jsx-curly-spacing": "off",
    "react/no-unescaped-entities": "off"
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
